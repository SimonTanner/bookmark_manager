<h1>Bookmark Manager</h1>
<p></p>
<h2>Bookmarks</h2>

<ul id='links'>
  <% @links.each do |link| %>
  <li>
    Title: <%= link.title %>
    URL: <%= link.url %>
    Tag: <%= link.tags %>
  </li>
  <% end %>
</ul>
<form action='/tags' method='get'>
  <div class=button>
    <button type='submit' value='tags'>View tags</button>
  </div>
</form>
